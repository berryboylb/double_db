    "express-rate-limit": "^6.7.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.5.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
